
<div class="container">
	<h3>
		MDN/Phone Status
	</h3>
	<br>
	
	<div>
		<p>
			If the table is empty, then the number you entered was not a valid PagePlus number
		</p>
	</div>
	<table class="table table-striped table-condensed">

		<% @phone_status_hash.each do |key, value| %>

			<tr>
				<td>
					<%=key.to_s.gsub("_"," ")%>
				</td>
				<td>
					<%=value%>
				</td>
			</tr>

		<% end %>

	</table>
</div>